            <Label style="-fx-font-size: 22px" text="Score4" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Button fx:id="playAgain" layoutX="130.0" layoutY="421.0" mnemonicParsing="false" onAction="#startNewGame" prefHeight="35.0" prefWidth="100.0" styleClass="button" text="Play again" />
      <Button fx:id="exit" layoutX="341.0" layoutY="421.0" mnemonicParsing="false" onAction="#exit" prefHeight="35.0" prefWidth="100.0" styleClass="button" text="Exit" />
   </children>
